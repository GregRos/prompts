{
  "name": "@gregros/prompts-exporter",
  "version": "1.0.0",
  "description": "Exporter for prompts workspace",
  "bugs": {
    "url": "https://github.com/GregRos/prompts/issues"
  },
  "repository": {
    "type": "git",
    "url": "github:GregRos/prompts",
    "directory": "packages/exporter"
  },
  "license": "MIT",
  "author": "Greg Rosenbaum",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/index.js",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "typings": "dist/index",
  "files": [
    "src",
    "dist",
    "package.json",
    "README.md",
    "!**/tsconfig*",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "tsc -b .",
    "build:clean": "run-s clean build",
    "clean": "shx rm -rf dist",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "watch": "tsc -b . --watch"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.20",
    "chokidar": "^4.0.3",
    "doddle": "^7.1.6",
    "front-matter": "^4.0.2",
    "fs-extra": "^11.3.2",
    "globby": "^15.0.0",
    "gpt-3-encoder": "^1.1.4",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "parjs": "^1.3.9",
    "path": "^0.12.7",
    "pathlib-js": "^1.5.0",
    "rxjs": "^7.8.2",
    "what-are-you": "^0.1.13",
    "yamprint": "^0.8.4"
  },
  "devDependencies": {
    "@swc/core": "^1.11.11",
    "@swc/jest": "^0.2.37",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.9.2",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.4",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "packageManager": "yarn@4.9.1",
  "engines": {
    "node": ">=16.0.0"
  }
}
