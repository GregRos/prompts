{
  "python.languageServer": "None",
  "typescript.preferences.preferTypeOnlyAutoImports": false,
  "files.associations": {
    "*.rules.md": "instructions",
    "**/*.rules/_.md": "instructions",
    "**/*.agent/_.md": "chatagent",
    "**/*.agent": "chatagent",
    "**/*.prompt/_.md": "prompt"
  },
  "files.exclude": {
    "node_modules": true,
    "dist": true,
    ".yarn": true,
    ".obsidian": true,
    "**/.space": true,
    "**/.obsidian": true,
    "**/.makemd": true,
    "**/.trash": true,
    ".out": true
  }
}
