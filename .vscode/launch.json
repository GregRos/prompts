{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run deploy.ts (tsx)",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npx",
      "runtimeArgs": ["tsx", "${workspaceFolder}/src/exporter/main.ts"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "env": {},
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Run deploy (dist JS)",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "node",
      "program": "${workspaceFolder}/dist/exporter/main.js",
      "runtimeArgs": ["--enable-source-maps", "--async-stack-traces"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "env": {},
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
